{
  "name": "Markdown-Writer",
  "main": "./lib/main",
  "version": "0.3.0",
  "description": "Atom as a Markdown blogging editor",
  "activationEvents": [
    "md-writer:new-post",
    "md-writer:insert-link",
    "md-writer:toggle-code-text",
    "md-writer:toggle-bold-text",
    "md-writer:toggle-italic-text",
    "md-writer:toggle-strikethrough-text",
    "md-writer:toggle-h1",
    "md-writer:toggle-h2",
    "md-writer:toggle-h3",
    "md-writer:toggle-h4",
    "md-writer:toggle-h5",
    "md-writer:manage-post-tags",
    "md-writer:manage-post-categories"
  ],
  "repository": "https://github.com/zhuochun/md-writer",
  "license": "MIT",
  "engines": {
    "atom": ">=0.50.0"
  },
  "dependencies": {
    "fs-plus": "2.x",
    "request": "2.x",
    "js-yaml": "3.x",
    "guid": "0.0.12",
    "season": "2.x"
  }
}
